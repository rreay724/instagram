                if (event.data == 'YOiWDQAABWb8LCTd') {
                    var iFrm = window.document.getElementById("x2_ad_x2_" + isn);
                    var amoObj = {};
                    amoObj["data"] = window['amo_YOiWDQAABWb8LCTd']
                    amoObj["mainHtmlFile"] = url;
                    amoObj["isn"] = "YOiWDQAABWb8LCTd";
